{
   "plugins": [
      "react"
   ],
   "parserOptions": {
      "ecmaVersion": "6",
      "sourceType": "module",
      "ecmaFeatures": {
         "jsx": true,
         "experimentalObjectRestSpread": true
      }
   },
   "rules": {
      "jsx-quotes": [2, "prefer-double"],
      "no-console": [2],
      "no-debugger": [2],
      "no-dupe-args": [2],
      "no-dupe-keys": [2],
      "no-duplicate-case": [2],
      "no-empty": [2],
      "no-extra-semi": [2],
      "no-func-assign": [2],
      "no-irregular-whitespace": [2],
      "no-negated-in-lhs": [2],
      "no-unexpected-multiline": [2],
      "no-unreachable": [2],
      "valid-jsdoc": [2],
      "valid-typeof": [2],
      "accessor-pairs": [2],
      "array-callback-return": [2],
      "block-scoped-var": [2],
      "curly": [2],
      "eqeqeq": [2],
      "no-alert": [2],
      "no-empty-function": [2],
      "no-empty-pattern": [2],
      "no-eq-null": [2],
      "no-eval": [2],
      "no-extra-bind": [2],
      "no-fallthrough": [2],
      "no-implied-eval": [2],
      "no-invalid-this": [2],
      "no-lone-blocks": [2],
      "no-loop-func": [2],
      "no-multi-spaces": [2],
      "no-multi-str": [2],
      "no-native-reassign": [2],
      "no-new": [2],
      "no-new-wrappers": [2],
      "no-param-reassign": [2],
      "no-redeclare": [2],
      "no-self-assign": [2],
      "no-self-compare": [2],
      "no-unmodified-loop-condition": [2],
      "no-useless-call": [2],
      "no-useless-concat": [2],
      "no-useless-escape": [2],
      "yoda": [2],
      "no-shadow": [2],
      "no-use-before-define": [2],
      "array-bracket-spacing": [2],
      "block-spacing": [2],
      "camelcase": [2],
      "comma-spacing": [2],
      "comma-style": [2],
      "computed-property-spacing": [2],
      "func-style": [2],
      "id-blacklist": [2],
      "indent": [2, 4, {"SwitchCase": 1}],
      "key-spacing": [2,{
        "align": {
          "beforeColon": true,
          "afterColon": true,
          "on": "colon"
        }
      }],
      "keyword-spacing": [2],
      "linebreak-style": [2],
      "lines-around-comment": [2],
      "max-depth": [2],
      "max-nested-callbacks": [2],
      "max-statements-per-line": [2],
      "new-cap": [2],
      "new-parens": [2],
      "no-inline-comments": [1],
      "no-lonely-if": [2],
      "no-mixed-spaces-and-tabs": [2],
      "object-curly-spacing": [2, "always"],
      "object-property-newline": [1],
      "quotes": [2, "double"],
      "require-jsdoc": [2],
      "semi": [2, "always"],
      "space-infix-ops": [2],
      "space-unary-ops": [2],
      "spaced-comment": [2],
      "arrow-spacing": [2],
      "constructor-super": [2],
      "generator-star-spacing": [2],
      "no-class-assign": [2],
      "no-const-assign": [2],
      "no-dupe-class-members": [2],
      "no-duplicate-imports": [2],
      "no-new-symbol": [2],
      "no-this-before-super": [2],
      "no-useless-computed-key": [2],
      "no-useless-constructor": [2],
      "no-useless-rename": [2],
      "no-var": [2],
      "prefer-arrow-callback": [2],
      "prefer-const": [2],
      "prefer-template": [1],
      "require-yield": [2],
      "sort-imports": [2],
      "yield-star-spacing": [2]
   }
}
